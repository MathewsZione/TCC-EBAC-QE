{
  "name": "ui",
  "version": "1.0.0",
  "description": "Projeto de automação UI",
  "main": "index.js",
  "scripts": {
    "test": "npm run cypress:run && npm run allure:generate",
    "cy:run": "cypress run --browser electron",
    "cy:run_produtos": "npx cypress run --spec cypress/e2e/catalogoDeProdutos.cy.js",
    "lint": "eslint cypress/**/*.js && eslint cypress.config.js",
    "lint:fix": "eslint cypress/**/*.js --fix && eslint cypress.config.js --fix",
    "allure:generate": "allure generate cypress/reports/allure-results --clean -o cypress/reports/allure-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "^13.11.0",
    "cypress-real-events": "^1.12.0",
    "faker-br": "^0.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "allure-commandline": "^2.29.0",
    "eslint": "^9.6.0",
    "eslint-plugin-cypress": "^2.13.3",
    "globals": "^15.8.0"
  }
}
